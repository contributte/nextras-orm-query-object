{
  "name": "contributte/nextras-orm-query-object",
  "description": "QueryObjects for Nextras ORM",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/nextras-orm-query-object",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": "^7.2",
    "nextras/dbal": "^3.1.1"
  },
  "require-dev": {
    "ninjify/qa": "^0.10.0",
    "ninjify/nunjuck": "^0.3.0",
    "nette/di": "~3.0.1",
    "nextras/orm": "^3.1.0",
    "mockery/mockery": "^1.3.0",
    "phpstan/phpstan": "^0.12.0",
    "phpstan/phpstan-strict-rules": "^0.12.0",
    "phpstan/phpstan-deprecation-rules": "^0.12.0",
    "phpstan/phpstan-nette": "^0.12.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Nextras\\Orm\\QueryObject\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Mocks\\": "tests/mocks"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.x-dev"
    }
  }
}
